<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            category="gov.redhawk.ui.category.sca"
            class="gov.redhawk.internal.ui.port.nxmplot.view.PlotView"
            icon="icons/SDU.gif"
            id="gov.redhawk.ui.port.nxmplot.portdata"
            name="Port Data"
            restorable="false">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="gov.redhawk.internal.ui.port.nxmplot.handlers.NewNxmPlotHandler"
            commandId="gov.redhawk.ui.port.nxmplot.newdataplot">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="gov.redhawk.ui.port.nxmplot.newdataplot"
            name="New Plot Connection">
         <commandParameter
               id="gov.redhawk.ui.port.nxmplot.newdataplot.fftparam"
               name="gov.redhawk.ui.port.nxmplot.newdataplot.fftparam"
               optional="false">
         </commandParameter>
      </command>
      <command
            categoryId="gov.redhawk.ui.commands.category.sca"
            defaultHandler="gov.redhawk.internal.ui.port.nxmplot.handlers.PlotUsesPortHandler"
            id="gov.redhawk.ui.port.nxmplot.command.plot"
            name="Plot Port">
         <commandParameter
               id="gov.redhawk.ui.port.nxmplot.command.plot.fftparam"
               name="gov.redhawk.ui.port.nxmplot.command.plot.fftparam"
               optional="false">
         </commandParameter>
      </command>
      
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:gov.redhawk.ui.port.nxmplot.portdata?after=additions">
         <command
               commandId="gov.redhawk.ui.port.nxmplot.newdataplot"
               id="gov.redhawk.ui.port.nxmplot.newdataplot"
               label="New Data Plot"
               style="push">
            <parameter
                  name="gov.redhawk.ui.port.nxmplot.newdataplot.fftparam"
                  value="false">
            </parameter>
         </command>
         <command
               commandId="gov.redhawk.ui.port.nxmplot.newdataplot"
               id="gov.redhawk.ui.port.nxmplot.newdataplot"
               label="New FFT Plot"
               style="push">
            <parameter
                  name="gov.redhawk.ui.port.nxmplot.newdataplot.fftparam"
                  value="true">
            </parameter>
         </command>
      </menuContribution>
      <!--menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
