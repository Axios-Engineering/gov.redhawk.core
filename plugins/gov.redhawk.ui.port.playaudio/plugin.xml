<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="gov.redhawk.ui.category.sca"
            class="gov.redhawk.ui.port.playaudio.views.View"
            id="gov.redhawk.ui.port.playaudio.view"
            name="%view.name">
      </view>
   </extension>
   <extension
         point="gov.redhawk.ui.portHandler">
      <portHandler
            class="gov.redhawk.ui.port.playaudio.internal.handlers.PlayPortHandler"
            id="gov.redhawk.ui.port.playaudio.play">
         <portInterface
               dataType="IDL:BULKIO/dataOctet:1.0">
         </portInterface>
         <portInterface
               dataType="IDL:BULKIO/dataChar:1.0">
         </portInterface>
         <portInterface
               dataType="IDL:BULKIO/dataDouble:1.0">
         </portInterface>
         <portInterface
               dataType="IDL:BULKIO/dataFloat:1.0">
         </portInterface>
         <portInterface
               dataType="IDL:BULKIO/dataShort:1.0">
         </portInterface>
         <portInterface
               dataType="IDL:BULKIO/dataUlong:1.0">
         </portInterface>
      </portHandler>
   </extension>

</plugin>
