<?eclipse version="3.4"?>
<plugin>
   <extension-point id="corbaViewParts" name="Corba Viewer Parts" schema="schema/corbaViewParts.exsd"/>
   <extension-point id="scaContentTypes" name="SCA Content Types" schema="schema/scaContentTypes.exsd"/>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            activeByDefault="true"
            contentProvider="gov.redhawk.sca.ui.ScaContentProvider"
            icon="icons/obj16/sca_explorer.png"
            id="gov.redhawk.ui.navigatorContent"
            labelProvider="gov.redhawk.sca.ui.ScaLabelProvider"
            name="SCA"
            priority="lower"
            providesSaveables="false"
            sortOnly="false">
         <actionProvider
               class="gov.redhawk.sca.internal.ui.actions.OpenWithActionProvider"
               id="gov.redhawk.ui.commonActionProvider">
            <enablement>
               <or>
               <and>
                  <instanceof
                        value="org.eclipse.emf.ecore.EObject">
                  </instanceof>
                  <test
                        forcePluginActivation="true"
                        property="org.eclipse.emf.ecore.namespace"
                        value="http://www.redhawk.gov/model/sca/2.0.0">
                  </test>
               </and>
               <instanceof
                     value="gov.redhawk.model.sca.provider.ScaWaveformsContainerItemProvider">
               </instanceof>
               <instanceof
                     value="gov.redhawk.model.sca.provider.ScaWaveformFactoriesContainerItemProvider">
               </instanceof>
               <instanceof
                     value="gov.redhawk.model.sca.provider.ScaDeviceManagersContainerItemProvider">
               </instanceof>
            </or>
            </enablement>
         </actionProvider>
         <triggerPoints>
            <instanceof
                  value="gov.redhawk.model.sca.ScaDomainManagerRegistry">
            </instanceof>
         </triggerPoints>
         <possibleChildren>
            <or>
               <and>
                  <instanceof
                        value="org.eclipse.emf.ecore.EObject">
                  </instanceof>
                  <test
                        forcePluginActivation="true"
                        property="org.eclipse.emf.ecore.namespace"
                        value="http://www.redhawk.gov/model/sca/2.0.0">
                  </test>
               </and>
               <instanceof
                     value="gov.redhawk.model.sca.provider.ScaWaveformsContainerItemProvider">
               </instanceof>
               <instanceof
                     value="gov.redhawk.model.sca.provider.ScaWaveformFactoriesContainerItemProvider">
               </instanceof>
               <instanceof
                     value="gov.redhawk.model.sca.provider.ScaDeviceManagersContainerItemProvider">
               </instanceof>
               <adapt
                     type="org.eclipse.core.resources.IResource">
               </adapt>
            </or>
         </possibleChildren>
         <commonSorter
               class="gov.redhawk.sca.internal.ui.ScaViewerSorter">
         </commonSorter>
         <dropAssistant
               class="gov.redhawk.sca.internal.ui.actions.ResourceDropAdapterAssistant"
               id="gov.redhawk.sca.ui.dropAssistant.resource">
            <possibleDropTargets>
               <or>
                  <instanceof
                        value="gov.redhawk.model.sca.ScaFileStore">
                  </instanceof>
                  <instanceof
                        value="org.eclipse.core.resources.IResource">
                  </instanceof>
               </or></possibleDropTargets>
         </dropAssistant>
      </navigatorContent>
      <commonFilter
            activeByDefault="true"
            description="Hides waveform factories"
            id="gov.redhawk.ui.filter.waveform_factories"
            name="Waveform Factories">
         <filterExpression>
            <instanceof
                  value="gov.redhawk.model.sca.provider.ScaWaveformFactoriesContainerItemProvider">
            </instanceof>
         </filterExpression>
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            description="Hides Waveform Components"
            id="gov.redhawk.ui.filter.waveform_components"
            name="Waveform Components">
         <filterExpression>
            <instanceof
                  value="gov.redhawk.model.sca.provider.ScaWaveformsContainerItemProvider">
            </instanceof>
         </filterExpression>
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            description="Hides device managers"
            id="gov.redhawk.ui.filter.device_managers"
            name="Device Managers">
         <filterExpression>
            <instanceof
                  value="gov.redhawk.model.sca.provider.ScaDeviceManagersContainerItemProvider">
            </instanceof>
         </filterExpression>
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            description="Hides domain file managers"
            id="gov.redhawk.ui.filter.domain_file_managers"
            name="Domain File Managers">
         <filterExpression>
            <instanceof
                  value="gov.redhawk.model.sca.ScaFileManager">
            </instanceof>
         </filterExpression>
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            class="gov.redhawk.sca.internal.ui.filters.DeviceManagerChildDevicesFilter"
            description="Hides child devices from being displayed under the device manager."
            id="gov.redhawk.ui.filter.child_devices"
            name="Device Manager Child Devices">
      </commonFilter>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
            viewerId="gov.redhawk.ui.sca_explorer">
         <includes>
            <contentExtension
                  isRoot="true"
                  pattern="gov.redhawk.ui.navigatorContent">
            </contentExtension>
            <contentExtension
                  pattern="gov.redhawk.ui.filter.*">
            </contentExtension>
            <contentExtension
                  pattern="gov.redhawk.ui.drop.*">
            </contentExtension>
		</includes>
      </viewerContentBinding>
      <viewer
            viewerId="gov.redhawk.ui.sca_explorer">
            <popupMenu
                allowsPlatformContributions="true"
                id="gov.redhawk.ui.sca_explorer#PopupMenu">  
             <insertionPoint name="group.new"/>
             <insertionPoint
                   name="group.open"
                   separator="true"/>
             <insertionPoint name="group.openWith"/>
             <insertionPoint
                   name="group.control"
                   separator="true">
             </insertionPoint>   
             <insertionPoint name="group.edit"
                   separator="true"/>   
             <insertionPoint name="group.reorganize" />         
             <insertionPoint
                   name="group.port"
                   separator="true"/>     
             <insertionPoint
                   name="group.build"
                   separator="true"/> 
             <insertionPoint
                   name="group.generate"
                   separator="true"/> 
             <insertionPoint
                   name="group.search"
                   separator="true"/>              
             <insertionPoint
                   name="additions"
                   separator="true"/>              
             <insertionPoint
                   name="group.properties"
                   separator="true"/>
                     </popupMenu>
            <options>
               <property
                     name="org.eclipse.ui.navigator.hideLinkWithEditorAction"
                     value="true">
               </property>
               <property
                     name="org.eclipse.ui.navigator.hideAvailableExtensionsTab"
                     value="true">
               </property>
            </options>
      </viewer>
      <viewerContentBinding
            viewerId="org.eclipse.ui.navigator.ProjectExplorer">
         <includes>
            <contentExtension
                  pattern="gov.redhawk.ui.navigatorContent">
            </contentExtension>
         </includes>
      </viewerContentBinding>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="gov.redhawk.ui.category.sca"
            name="SCA">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
