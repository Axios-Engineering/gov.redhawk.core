   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=group.control">
         <command
               commandId="gov.redhawk.ui.command.connect"
               icon="icons/clcl16/connect.png"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="gov.redhawk.ui.command.disconnect"
               icon="icons/clcl16/disconnect.png"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="gov.redhawk.ui.command.resource.start"
               icon="icons/clcl16/play.gif"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false">
                  <adapt
                        type="CF.ResourceOperations">
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
         <command
               commandId="gov.redhawk.ui.command.resource.stop"
               icon="icons/clcl16/stop.png"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false">
                  <adapt
                        type="CF.ResourceOperations">
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:gov.redhawk.ui.sca_explorer?after=additions">
         <command
               commandId="gov.redhawk.ui.command.new_domain"
               icon="icons/clcl16/add.gif"
               id="gov.redhawk.ui.menucontribution.new_domain"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=group.new">
         <command
               commandId="gov.redhawk.ui.command.waveform.install"
               icon="icons/clcl16/install.gif"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false">
                  <instanceof
                        value="gov.redhawk.model.sca.provider.ScaWaveformFactoriesContainerItemProvider">
                  </instanceof>
               </iterate>
            </visibleWhen>
         </command>
         <command
               commandId="gov.redhawk.ui.command.waveform.launch"
               icon="icons/clcl16/new_waveform.gif"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false">
                  <or>
                     <instanceof
                           value="gov.redhawk.model.sca.ScaDomainManager">
                     </instanceof>
                     <instanceof
                           value="gov.redhawk.model.sca.provider.ScaWaveformFactoriesContainerItemProvider">
                     </instanceof>
                     <instanceof
                           value="gov.redhawk.model.sca.provider.ScaWaveformsContainerItemProvider">
                     </instanceof>
                     <instanceof
                           value="gov.redhawk.model.sca.ScaWaveformFactory">
                     </instanceof>
                  </or>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=group.edit">
         <command
               commandId="org.eclipse.ui.edit.delete"
               label="Uninstall"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false">
                  <instanceof
                        value="gov.redhawk.model.sca.ScaWaveformFactory">
                  </instanceof>
               </iterate>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.edit.delete"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false">
                  <or>
                     <instanceof
                           value="gov.redhawk.model.sca.ScaDomainManager">
                     </instanceof>
                     <instanceof
                           value="gov.redhawk.model.sca.ScaFileStore">
                     </instanceof>
                  </or>
               </iterate>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.edit.delete"
               label="Release"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false">
                  <adapt
                        type="CF.LifeCycleOperations">
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.edit.delete"
               label="Shutdown"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false">
                  <adapt
                        type="CF.DeviceManagerOperations">
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.edit.delete"
               label="Disconnect"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <instanceof
                        value="gov.redhawk.model.sca.ScaConnection">
                  </instanceof>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?before=group.control">
         <separator
               name="gov.redhawk.sca.ui.separator.refresh"
               visible="true">
         </separator>
         <command
               commandId="org.eclipse.ui.file.refresh"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <adapt
                        type="gov.redhawk.model.sca.IRefreshable">
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.RemoveDomainHandler"
            commandId="org.eclipse.ui.edit.delete">
         <activeWhen>
            <iterate
                  ifEmpty="false"
                  operator="or">
               <instanceof
                     value="gov.redhawk.model.sca.ScaDomainManager">
               </instanceof>
            </iterate>
         </activeWhen>
      </handler>
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.ReleaseHandler"
            commandId="org.eclipse.ui.edit.delete">
         <activeWhen>
            <iterate
                  ifEmpty="false"
                  operator="or">
               <adapt
                     type="CF.LifeCycleOperations">
               </adapt>
            </iterate>
         </activeWhen>
      </handler>
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.ShutdownNodeHandler"
            commandId="org.eclipse.ui.edit.delete">
         <activeWhen>
            <iterate
                  ifEmpty="false"
                  operator="or">
               <adapt
                     type="CF.DeviceManagerOperations">
               </adapt>
            </iterate>
         </activeWhen>
      </handler>
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.UninstallApplication"
            commandId="org.eclipse.ui.edit.delete">
         <activeWhen>
            <iterate
                  ifEmpty="false"
                  operator="or">
               <instanceof
                     value="gov.redhawk.model.sca.ScaWaveformFactory">
               </instanceof>
            </iterate>
         </activeWhen>
      </handler>
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.ConnectDomainHandler"
            commandId="gov.redhawk.ui.command.connect">
         <activeWhen>
            <iterate
                  ifEmpty="false"
                  operator="and">
               <instanceof
                     value="gov.redhawk.model.sca.ScaDomainManager">
               </instanceof>
            </iterate>
         </activeWhen>
      </handler>
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.DisconnectDomainHandler"
            commandId="gov.redhawk.ui.command.disconnect">
         <activeWhen>
            <iterate
                  ifEmpty="false"
                  operator="and">
               <instanceof
                     value="gov.redhawk.model.sca.ScaDomainManager">
               </instanceof>
            </iterate>
         </activeWhen>
      </handler>
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.ConnectPortHandler"
            commandId="gov.redhawk.ui.command.connect">
         <activeWhen>
            <and>
               <count
                     value="2">
               </count>
               <and>
                  <iterate
                        ifEmpty="false"
                        operator="or">
                     <instanceof
                           value="gov.redhawk.model.sca.ScaUsesPort">
                     </instanceof>
                  </iterate>
                  <iterate
                        ifEmpty="false"
                        operator="or">
                     <or>
                        <instanceof
                              value="gov.redhawk.model.sca.ScaProvidesPort">
                        </instanceof>
                        <instanceof
                              value="gov.redhawk.model.sca.ScaComponent">
                        </instanceof>
                     </or></iterate></and>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.DisconnectPortHandler"
            commandId="org.eclipse.ui.edit.delete">
         <activeWhen>
            <iterate
                  ifEmpty="false"
                  operator="or">
               <instanceof
                     value="gov.redhawk.model.sca.ScaConnection">
               </instanceof>
            </iterate>
         </activeWhen>
      </handler>
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.RefreshHandler"
            commandId="org.eclipse.ui.file.refresh">
         <activeWhen>
            <iterate
                  ifEmpty="false"
                  operator="and">
               <adapt
                     type="gov.redhawk.model.sca.IRefreshable">
               </adapt>
            </iterate>
         </activeWhen>
         <enabledWhen>
            <iterate
                  ifEmpty="false"
                  operator="and">
               <adapt
                     type="gov.redhawk.model.sca.IRefreshable">
               </adapt>
            </iterate>
         </enabledWhen>
      </handler>
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.DeleteScaFileStoreHandler"
            commandId="org.eclipse.ui.edit.delete">
