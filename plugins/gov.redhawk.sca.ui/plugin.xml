   <extension
         point="org.eclipse.ui.commands">
      <category
            id="gov.redhawk.ui.commands.category.sca"
            name="SCA">
      </category>
      <command
            categoryId="gov.redhawk.ui.commands.category.sca"
            defaultHandler="gov.redhawk.sca.internal.ui.handlers.InstallApplication"
            id="gov.redhawk.ui.command.waveform.install"
            name="Install Waveform...">
      </command>
      <command
            categoryId="gov.redhawk.ui.commands.category.sca"
