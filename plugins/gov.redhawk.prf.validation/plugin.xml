<?eclipse version="3.4"?>
<plugin>
<extension
         point="org.eclipse.emf.validation.constraintProviders">
      <constraintProvider
            cache="true">
         <package
               namespaceUri="http://sca.jpeojtrs.mil/prf">
         </package>
         <constraints
               categories="gov.redhawk.validation.category">
            <constraint
                  class="gov.redhawk.validation.prf.DuplicatePropertyNameConstraint"
                  id="gov.redhawk.validation.constraint.prf.dupName"
                  isEnabledByDefault="true"
                  lang="java"
                  mode="Live"
                  name="%constraint.name"
                  severity="WARNING"
                  statusCode="2">
               <message>
                  %constraint.message
               </message>
               <target
                     class="Simple">
                  <event
                        name="Set">
                     <feature
                           name="name">
                     </feature>
                  </event>
               </target>
               <target
                     class="SimpleSequence">
                  <event
                        name="Set">
                     <feature
                           name="name">
                     </feature>
                  </event>
               </target>
               <target
                     class="Struct">
                  <event
                        name="Set">
                     <feature
                           name="name">
                     </feature>
                  </event>
               </target>
               <target
                     class="StructSequence">
                  <event
                        name="Set">
                     <feature
                           name="name">
                     </feature>
                  </event>
               </target>
            </constraint>
            <constraint
                  class="gov.redhawk.validation.prf.ValidValueTypeConstraint"
                  id="gov.redhawk.prf.validation.constraint.ValueType"
                  isEnabledByDefault="true"
                  lang="java"
                  mode="Live"
                  name="%constraint.name.0"
                  severity="ERROR"
                  statusCode="1">
               <message>
                  %constraint.message.0
               </message>
               <target
                     class="Simple">
                  <event
                        name="Set">
                     <feature
                           name="value">
                     </feature>
                  </event>
               </target>
               <target
                     class="Values">
                  <event
                        name="Add">
                     <feature
                           name="value">
                     </feature>
                  </event>
                  <event
                        name="Add Many">
                     <feature
                           name="value">
                     </feature>
                  </event>
                  <event
                        name="Remove">
                     <feature
                           name="value">
                     </feature>
                  </event>
                  <event
                        name="Remove Many">
                     <feature
                           name="value">
                     </feature>
                  </event>
               </target>
               <target
                     class="SimpleRef">
                  <event
                        name="Set">
                     <feature
                           name="value">
                     </feature>
                  </event>
               </target>
            </constraint>
            <constraint
                  class="gov.redhawk.validation.prf.ValidEnumValueConstraint"
                  id="gov.redhawk.prf.validation.constraint.ValidEnumValue"
                  isEnabledByDefault="true"
                  lang="java"
                  mode="Live"
                  name="%constraint.name.1"
                  severity="ERROR"
